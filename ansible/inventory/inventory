[loadbalancers]
lb1 ansible_host=lb1 ansible_ssh_user=root ansible_ssh_common_args='-o StrictHostKeyChecking=no'
lb2 ansible_host=lb2 ansible_ssh_user=root ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[backends]
backend1 ansible_host=backend1 ansible_ssh_user=root ansible_ssh_common_args='-o StrictHostKeyChecking=no'
backend2 ansible_host=backend2 ansible_ssh_user=root ansible_ssh_common_args='-o StrictHostKeyChecking=no'

